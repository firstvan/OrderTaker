<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane id="AnchorPane" prefHeight="153.0" prefWidth="321.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.firstvan.controller.DatabaseController">
    <children>
        <Label layoutX="14.0" layoutY="37.0" minHeight="16" minWidth="69" text="User name"/>
        <Button fx:id="jdbc_login" disable="true" layoutX="231.0" layoutY="116.0" mnemonicParsing="false"
                onAction="#LogInAction" text="Log in"/>
        <Button fx:id="home_exit" layoutX="181.0" layoutY="116.0" mnemonicParsing="false" onAction="#ExitAction"
                text="Exit"/>
        <TextField fx:id="jdbc_user" layoutX="95.0" layoutY="32.0" onKeyTyped="#userField"/>
        <Label layoutX="14.0" layoutY="72.0" text="Password"/>
        <PasswordField fx:id="jdbc_pass" layoutX="95.0" layoutY="67.0" onAction="#passwordField"
                       onKeyTyped="#passwordField"/>
    </children>
</AnchorPane>
